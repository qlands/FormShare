<div class="row border-bottom {% if activeUser == None %}white-bg{% endif %}">
    {% if activeUser != None %}
        <nav class="navbar navbar-fixed-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                {% block header_main_button %}
                    <a class="navbar-minimalize minimalize-styl-2" href="#"><img alt="image" src="{{ request.url_for_static('formshare-small-black.png') }}"/> Form<b>Share</b></a>
{#                    <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>#}
                {% endblock header_main_button %}
                {% block header_search %}
                    {% if activeUser != None %}
{#                        {% include 'dashboard/snippets/header_search.jinja2' %}#}
                    {% endif %}
                {% endblock header_search %}
            </div>
            <ul class="nav navbar-top-links navbar-right">
                {% block header_top_links %}
                    {% block header_top_welcome %}
                        {% if showWelcome %}
                            <li style="padding: 20px">
                                <span class="m-r-sm text-muted welcome-message">{{ _('Welcome to FormShare') }}</span>
                            </li>
                        {% endif %}
                    {% endblock header_top_welcome %}
                    {% block header_messages %}
                        {% if activeUser != None %}
{#                            {% include 'dashboard/snippets/header_messages.jinja2' %}#}
                        {% endif %}
                    {% endblock header_messages %}
                    {% block header_notifications %}
                        {% if activeUser != None %}
{#                            {% include 'dashboard/snippets/header_notifications.jinja2' %}#}
                        {% endif %}
                    {% endblock header_notifications %}

                    {% block header_timezone %}
                        {% if request.h.get_setting(request,'use_timezones','False') == 'True' %}
                            <li>
                                <div>
                                    {{ _('Time zone') }}:
                                    <select id="timezone" style="margin-right: 10px">
                                        {% block timezones_options %}
                                            <option id="tz_formshare" value="formshare">{{ _("FormShare's") }}: {{ system_timezone }}{% if system_timezone != "UTC" %} (UTC{{ system_timezone_offset }}){% endif %}</option>
                                            <option id="tz_user" value="user" title="{{ user_timezone.timezone_name }}">{{ _('Yours') }} {% if user_timezone.user_timezone != "UTC" %} (UTC{{ user_timezone.timezone_utc_offset }}){% endif %}</option>
                                            <option id="tz_project" value="project">{{ _('By project') }}</option>
                                        {% endblock timezones_options %}
                                    </select>
                                </div>
                            </li>
                        {% endif %}
                    {% endblock header_timezone %}

                    {% block header_language %}
                    <li>
                        <div>
                            {{ _('Language') }}:
                            <select id="language">
                                {% block language_options %}
                                    <option id="en" value="en">English</option>
                                    <option id="es" value="es">Español</option>
                                    <option id="fr" value="fr">Français</option>
                                    <option id="pt" value="pt">Português</option>
                                    <option id="uk" value="uk">Українська</option>
                                {% endblock language_options %}
                            </select>
                        </div>
                    </li>
                    {% endblock header_language %}

                    {% block header_logout %}
                        {% if activeUser != None %}
                            <li>
                                <a href="{{ request.route_url('logout') }}">
                                    <i class="fa fa-sign-out"></i> {{ _('Sign out') }}
                                </a>
                            </li>
                        {% endif %}
                    {% endblock header_logout %}
                    {% block header_sidebar_button %}
                        {% if activeUser != None %}
{#                            <li>#}
{#                                <a class="right-sidebar-toggle">#}
{#                                    <i class="fa fa-tasks"></i>#}
{#                                </a>#}
{#                            </li>#}
                        {% endif %}
                    {% endblock header_sidebar_button %}
                {% endblock header_top_links %}
            </ul>
        </nav>
    {% else %}
